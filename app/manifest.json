{
  "name": "Taskk",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Quickly add your tasks from Chrome!",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "taskk.html"
  },
  "permissions": [
    "https://api.taskk.it/"
  ]
}
